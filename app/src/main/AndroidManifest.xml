<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="pe.com.codespace.nurse" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".AnalyticsApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        tools:ignore="UnusedAttribute">

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".SplashScreenActivity"
            android:label="@string/app_name"
             >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Input2ParametrosActivity"
            android:label="@string/title_activity_none" >
        </activity>
        <activity
            android:name=".Input3ParametrosActivity"
            android:label="@string/title_activity_none" >
        </activity>
        <activity
            android:name=".Input4ParametrosActivity"
            android:label="@string/title_activity_none" >
        </activity>
        <activity
            android:name=".InputConvertSolutionActivity"
            android:label="@string/title_activity_none" >
        </activity>
        <activity
            android:name=".Escala3ParametrosActivity"
            android:label="@string/title_activity_none">
        </activity>
        <activity
            android:name=".Escala5ParametrosActivity"
            android:label="@string/title_activity_none" >
        </activity>
        <activity
            android:name=".Escala6ParametrosActivity"
            android:label="@string/title_activity_none">
        </activity>
        <activity
            android:name=".Escala10ParametrosActivity"
            android:label="@string/title_activity_none">
        </activity>
        <activity
            android:name=".EscalaNoParametrosActivity"
            android:label="@string/title_activity_none" >
        </activity>
        <activity
            android:name="com.google.android.gms.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"
            android:theme="@android:style/Theme.Translucent" />

        <activity
            android:name=".RegladetresActivity"
            android:label="@string/title_activity_none" >
        </activity>
        <activity
            android:name=".FechasActivity"
            android:label="@string/title_activity_none">
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".ApacheActivity"
            android:label="@string/title_activity_none">
        </activity>
        <activity
            android:name=".PreferencesActivity"
            android:label="@string/title_activity_none" >
        </activity>
    </application>

</manifest>
